   "source": [
    "from itertools import combinations\n",
    "from random import shuffle\n",
    "\n",
    "def comb(s):\n",
    "    perms = list(combinations(s, 5)) # create all permutations\n",
    "    \n",
    "    return [\"\".join(p) for p in perms]\n",
    "\n",
    "a = comb('ABCDEFGHIJ')\n",
    "f = open(\"output.txt\",'w')\n",
    "for i in a:\n",
    "    f.write(str(i)+\"\\n\")\n",
    "\n",
    "f.close()"
   ]
  }
